<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.walkntrade"
    android:versionCode="11"
    android:versionName="1.3.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <permission
        android:name="com.walkntrade.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.walkntrade.permission.C2D_MESSAGE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:logo="@drawable/walkntrade_icon"
        android:theme="@style/WalkntradeTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <receiver
            android:name="com.walkntrade.gcm.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.walkntrade" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.walkntrade.gcm.GcmIntentService"
            android:exported="false" >
        </service>

        <receiver
            android:name="com.walkntrade.gcm.NotificationBroadcastReceiver"
            android:exported="false" >
        </receiver>

        <activity
            android:name="com.walkntrade.Walkntrade_Main"
            android:label="@string/app_name"
            android:theme="@style/WalkntradeTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.walkntrade.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/WalkntradeTheme.NoActionBar" >
        </activity>
        <activity
            android:name="com.walkntrade.RegistrationActivity"
            android:label="@string/title_activity_registration" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.LoginActivity" />
        </activity>
        <activity
            android:name="com.walkntrade.Selector"
            android:label="@string/title_activity_selector"
            android:theme="@style/WalkntradeTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustPan" >
        </activity>
        <activity
            android:name="com.walkntrade.SchoolPage"
            android:label="@string/tSchoolName" >
        </activity>
        <activity
            android:name="com.walkntrade.ShowPage"
            android:label="@string/title_activity_show__page"
            android:windowSoftInputMode="stateHidden|adjustPan" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.SchoolPage" />

            <intent-filter>
                <action android:name="android.intent.action.SHOWPAGE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.walkntrade.ImageDialog"
            android:label="@string/title_activity_image_dialog"
            android:theme="@style/ImageDialogTheme" >
        </activity>
        <activity
            android:name="com.walkntrade.AddPost"
            android:label="@string/title_activity_add_post" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.SchoolPage" />
        </activity>
        <activity
            android:name="com.walkntrade.ResetPasswordActivity"
            android:label="@string/title_activity_reset_password"
            android:theme="@style/DialogTheme" >
        </activity>
        <activity
            android:name="com.walkntrade.VerifyKeyActivity"
            android:label="@string/title_activity_verify_key"
            android:theme="@style/DialogTheme" >
        </activity>
        <activity
            android:name="com.walkntrade.UserSettings"
            android:configChanges="orientation"
            android:label="@string/title_activity_user_settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.SchoolPage" />
        </activity>
        <activity
            android:name="com.walkntrade.ViewPosts"
            android:label="@string/title_activity_view_posts" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.UserSettings" />
        </activity>
        <activity
            android:name="com.walkntrade.ContactPref"
            android:label="@string/title_activity_contact_pref" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.UserSettings" />
        </activity>
        <activity
            android:name="com.walkntrade.UserAvatar"
            android:label="@string/title_activity_user_avatar" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.UserSettings" />
        </activity>
        <activity
            android:name="com.walkntrade.AccountSettings"
            android:label="@string/title_activity_user_account" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.UserSettings" />
        </activity>
        <activity
            android:name="com.walkntrade.AccountSettingsChange"
            android:label="@string/title_activity_user_account" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.AccountSettings" />
        </activity>
        <activity
            android:name="com.walkntrade.Feedback"
            android:label="@string/send_feedback" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.Privacy_Feedback" />
        </activity>
        <activity
            android:name="com.walkntrade.Messages"
            android:configChanges="orientation|screenSize"
            android:label="@string/title_activity_messages" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.SchoolPage" />
        </activity>
        <activity
            android:name="com.walkntrade.ShowMessage"
            android:label="@string/title_activity_show_message" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.Messages" />
        </activity>
        <activity
            android:name="com.walkntrade.NotificationSettings"
            android:label="@string/title_activity_notification_settings" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.ContactPref" />
        </activity>
        <activity
            android:name="com.walkntrade.Privacy_Feedback"
            android:label="@string/privacy_and_feedback" >
        </activity>
        <activity
            android:name=".EditPost"
            android:label="@string/title_activity_edit_post_page"
            android:parentActivityName="com.walkntrade.ViewPosts" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.walkntrade.ViewPosts" />
        </activity>
    </application>

</manifest>
